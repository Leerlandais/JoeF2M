<!DOCTYPE html>
<html lang="{% block lang %}fr{% endblock %}">
<head>
    {% block head %}
        {% block meta %}{% endblock %}
        <title>{% block title %}Title{% endblock %}</title>
        {% block stylesheet %}{% endblock %}
    {% endblock %}
</head>
<body class={% block bodyClass %}{% endblock %}>{% block body %}

    {% block navBar %}
        {% block connectBtn %} {% endblock %}
    {% endblock %}

    {% block content %}
        {% block hero %}
            {% block heroText %}{% endblock %}
            {% block heroImg  %}{% endblock %}
        {% endblock %}

        {% block presentation %}
            {% block presText %}{% endblock %}
            {% block presImg %}{% endblock %}
        {% endblock %}

        {% block formations %}{% endblock %}
        {% block projects %}{% endblock %}
        {% block contactForm %}{% endblock %}
        {% block partners %}{% endblock %}
        {% block avis %}{% endblock %}
        {% block autreOne %}{% endblock %} {# N'oublie pas de renommé ce block au cas ou de besoin #}
        {% block autreTwo %}{% endblock %} {# N'oublie pas de renommé ce block au cas ou de besoin #}
    {% endblock %}

    {% block footer %}{% endblock %}

{% block javascript %}{% endblock %}

</body> {% endblock %}
</html>